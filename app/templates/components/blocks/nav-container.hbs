{{yield}}
{{#unless visited}}
  <div class="cookie-policy-popup">
    <div class="cookie-policy-body">
      <p class="cookie-policy-text">
        By browsing Jobbio you agree to our Cookie Policy. We only use Cookies to enhance your Jobbio experience. Click {{#link-to 'cookie-policy' target='_blank' id="frontHeadCookiePolicy"}}here{{/link-to}} to view details.
      </p>
      <button id="frontHeadCompanyCloseButton" type="button" style="padding-left: 3.48214286em; padding-right: 3.48214286em; margin-top: 15px" class="btn btn--lg btn--primary type--uppercase" {{action "hideCookiePolicyPopup"}}>CLOSE</button>
    </div>
  </div>
{{/unless}}
<div id="scrolltop_" class="scrolltop-" style="background-color: {{channel.colour}};">
  <i class="stack-interface stack-up-open-big"></i>
</div>
<div class="nav-container front-head channel-nav" style="min-height: 0px;">
  <div class="via-1517474857094" via="via-1517474857094" vio="navigation (main)">
    <div class="bar bar--sm mobile-bar visible-xs">
      <div class="container">
        <div class="row">
          <div class="col-5 col-sm-2">
            <a href="#">
              <img class="logo logo-dark" alt="logo" src="{{{channelLogoDark}}}">
              <img class="logo logo-light" alt="logo" src="{{{channelLogoLight}}}">
            </a>
          </div>
          <div class="col-7 col-sm-10 text-right">
            <button id="frontHeadLogoMobile" {{action toggleProperty 'active'}} class="hamburger-toggle" data-toggle-class="#menu1;hidden-xs hidden-sm">
              <i class="icon icon--sm stack-interface stack-menu"></i>
            </button>
          </div>
        </div>
      </div>
    </div>
    <nav id="menu1" class="bar bar-1 {{if active 'hidden-sm'}} {{unless active 'hidden-xs'}} bar--absolute bar--transparent" data-scroll-class="366px:pos-fixed">
      <div class="container">
        <div class="row">
          <div class="col-md-1 col-sm-2 hidden-xs">
            <div class="bar__module">
              <a href="#">
                <img class="logo logo-dark" alt="logo" src="{{{channelLogoDark}}}">
                <img class="logo logo-light" alt="logo" src="{{{channelLogoLight}}}">
              </a>
            </div>
          </div>
          <div class="col-md-11 col-sm-12 text-right text-left-xs text-left-sm">
            <div class="bar__module">
              <ul class="menu-horizontal text-left">
                {{#each navLinks as |entry|}}
                  <li>
                    <span class="{{entry.icon}}"></span>
                    {{scroll-to href=(concat '#section-' entry.link.id) label=entry.title}}
                  </li>
                {{/each}}
              </ul>
            </div>
            <div class="bar__module">
              <a class="btn type--uppercase" href="/"> <span class="btn__text">Back to Jobbio</span></a>
            </div>
          </div>
        </div>
      </div>
    </nav>
  </div>
</div>